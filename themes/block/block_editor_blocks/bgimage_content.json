   "identifier" : "bgimage_content",
   "label" : "背景画像 + コンテンツ",
   "preview_header" : "<script>\r\ndocument.addEventListener(\"DOMContentLoaded\", async () => {\r\n  if (document.body.dataset.hasCompiledHtml) {\r\n    // 処理済み\r\n    return;\r\n  }\r\n\r\n  const imgContainerElm = document.querySelector(\".image\");\r\n  const imgElm = imgContainerElm.querySelector(\"img\");\r\n  const img = imgElm ? imgElm.src : \"\";\r\n  const textColorElm = document.querySelector(\".text-color\");\r\n  const m = textColorElm.textContent.match(/.*#([0-9a-fA-F]{1,2})([0-9a-fA-F]{1,2})([0-9a-fA-F]{1,2})/);\r\n  const textColorCss = m ? `color: #${m[1]}${m[2]}${m[3]}; ` : \"\";\r\n\r\n  imgContainerElm.remove();\r\n  textColorElm.remove();\r\n\r\n  const text = document.body.innerHTML.replace(/<!--\\s+\\/?mt-beb.*?-->/g, \"\");\r\n\r\n  MTBlockEditorSetCompiledHtml(`\r\n    <section class=\"bg-img\" style=\"${textColorCss}background-image: url(&quot;${img}&quot;)\">\r\n      <div class=\"inner-wrap\">\r\n          ${text}\r\n      </div>\r\n    </section>\r\n  `);\r\n});\r\n</script>",
   "show_preview" : true,
   "wrap_root_block" : false
